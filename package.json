  "scripts": {
    "init": "takeshape init",
    "clean": "rm -rf static/assets/javascripts && rm -rf static/assets/stylesheets && rm -rf static/assets/images && rm -f static/assets/*.json",
    "lint:js": "eslint src/javascripts",
    "lint:css": "stylelint src/stylesheets/**/*.scss --syntax scss",
    "lint": "npm-run-all -p lint:*",
    "imagemin": "imagemin src/images/* -o static/assets/images",
    "images:dev": "cp -R src/images static/assets/images",
    "serve": "node -e 'setTimeout(function() {}, 2000)' && browser-sync start --server build --port 5000 --files 'build/**/*, !build/sitemap.xml, !build/feed.xml' --reload-debounce",
    "takeshape:build": "takeshape build",
    "build:images": "npm run imagemin",
    "build:images:dev": "npm run images:dev",
    "build:webpack": "NODE_ENV=production webpack --display-error-details",
    "build:webpack:dev": "webpack --display-error-details -d",
    "build": "npm-run-all --serial clean build:images build:webpack",
    "deploy": "npm run build && takeshape deploy",
    "watch:js": "webpack --watch",
    "watch:images": "chokidar 'src/images/**/*' -d -c 'npm run build:images'",
    "watch:takeshape": "takeshape watch",
    "watch": "npm-run-all --serial clean build:images:dev build:webpack:dev takeshape:build && npm-run-all --continue-on-error --parallel watch:* serve",
    "start": "npm run watch"
  },
